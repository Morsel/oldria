<% form_for [:admin, @date_range] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :start_date %>
    <%= f.date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %>
    <%= f.date_select :end_date %>
  </p>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name, :class => 'text' %>
  </p>
<p><p><%= link_to "Back to List", admin_date_ranges_path, :class => 'button' %></p>
  <p><%= button_tag "Save" %></p>
<% end %>
