<%= title "Twitter archive for #{@restaurant.name}", false %>

<div id="restaurant_profile_view">
    <%= render :partial => "restaurant_header" %>

	<h2>Twitter updates</h2>
    <% for post in @restaurant.twitter_client.home_timeline.each do %>
    <p><%= link_to post.text, "http://twitter.com/#{@restaurant.twitter_username}/status/#{post.id}" %></p>
    <% end %>
</div>