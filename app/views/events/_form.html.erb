<% semantic_form_for [restaurant, event] do |f| %>
    <% f.inputs do %>
        <%= f.input :title %>
        <%= f.input :start_at, :label => "Start Date" %>
        <%= f.input :end_at, :label => "End Date" %>
        <%= f.input :location %>
        <%= f.input :description %>
    <% end %>
    
    <p>
      <%= button_tag "Save", :name => 'submit' %>
    </p>
<% end %>