
<%#= link_to "New Slide", new_admin_sf_slide_path, :class => "button positive" %>


<% title "Spoonfeed Slides" %>

<%= table_for @slides do |t| %>
  <%= t.head do %>
    <%= t.r do %>
      <%= t.h('Image') %>
      <%= t.h('Photo Credit') %>
      <%= t.h('Title') %>
      <%= t.h('Excerpt') %>
      <%= t.h('Link') %>
      <th></th>
      <th></th>
    <% end %>
  <% end %>
  <%= t.body do |slide| %>
    <%= t.r do %>
      <%= t.d(h(slide.image.url(:thumb))) %>
      <%= t.d(h(slide.photo_credit)) %>
      <%= t.d(h(slide.title)) %>
      <%= t.d(h(slide.excerpt)) %>
      <%= t.d(h(slide.link)) %>
  <% end %>
<% end %>
<% end -%>