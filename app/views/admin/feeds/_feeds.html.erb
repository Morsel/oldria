<%= table_for(feeds, :class => 'feeds') do |t| %>
 <% t.data :actions => [:edit, :destroy], :action_prefix => 'admin' do
      t.cell(:handle, :heading => '', :cell_html => {:class => 'small handle'}) { "[drag]" }
      t.cell :title
      t.cell :url
      t.cell :feed_url
      t.cell :featured, :cell_html => { :class => 'featured' }
    end %>
<% end -%>
